package Boletin1;

public class EjemploJava {

	public static void main(String[] args) {
		int numero = 6;
		System.out.println("numero");
		String cadena = "Hola";
		System.out.println(cadena+ "el numero es el" +numero);
		//final int codigoPostal = 41210
		int a =1;
		System.out.println(a);
		int b = a++; // b= a; a = a+1;
		System.out.println (b);
		int c = a --; //c= a; a = a-1;
		System.out.println(c);
		System.out.println(a);
		
	}

}